<Window x:Class="Semana2A.Clientes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Semana2A"
        mc:Ignorable="d"
        Title="Clientes" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <Button Click="clientes_Click" Margin="5" Width="130" Height="20">LISTAR CLIENTES</Button>
                <Button Click="insertar_Click" Margin="5" Width="130" Height="20">INSERTAR CLIENTE</Button>
                <Button Click="modificar_Click" Margin="5" Width="130" Height="20">MODIDICAR CLIENTE</Button>
                <Button Click="eliminar_Click" Margin="5" Width="130" Height="20">ELIMINAR CLIENTE</Button>
            </StackPanel>
            <DataGrid x:Name="brayanTable" Height="400"  Width="720"/>
        </StackPanel>
    </Grid>
</Window>
